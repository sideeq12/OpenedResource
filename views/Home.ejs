<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>OpenedResources</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="/style.css" type="text/css">
</head>
<body>
      <div class="jumbotron">
          <h3>OpenedResources</h3>
      </div>

      <div class="container">
          <div class="news">
                <h3>Latest Stock news</h5>
        
                        <h5 style="color: #03506f;"><%= title %></h5>
                        <p>
                            <%= message %>
                            <br/>StockTickers : <% tickers.forEach((Symbol)=>{ %>
                                <span><%= Symbol.symbol %></span>
                            <% }) %>
                        </p>
                        <a href="<%= url %>" class="btn btn-primary">Read more</a>
                        
           
                
          </div>
          <hr/>
          <div  style="width: 100%; height:500px;">
              <div class="col-sm-12 Release">
                        <h4>CRYPTO RATING (FCAS)</h4>
                        <table>
                            <thead>
                                <th>Symbol</th>
                                <th>Name</th>
                                <th>Fcas rating</th>
                                <th>Fcas score</th>
                                <th>Developer score</th>
                                <th>Market maturity score</th>
                                <th>Utility score </th>
                            </thead>
                            <tbody>
                             <%   dataArray.forEach((inner)=>{ %>
                                <tr>
                                    <td><%= inner["1. symbol"] %></td>
                                    <td><%= inner["2. name"] %></td>
                                    <td><%= inner["3. fcas rating"] %></td>
                                    <td><%= inner["4. fcas score"] %></td>
                                    <td><%= inner["5. developer score"] %></td>
                                    <td><%= inner["6. market maturity score"] %></td>
                                    <td><%= inner["7. utility score"] %></td>
                                </tr>
                                
                               <%  }) %>
                            </tbody>
                        </table>
                        
              </div>
              <div class="historical" >
            <h4>Historical Data</h4>
            <div class="Market">
                <h6>AMRN : AMARIN Corporation plc.</h6>
                <table>
                    <thead>
                        <th>Date</th>
                        <th>Open</th>
                        <th>High</th>
                        <th>Low</th>
                        <th>Close</th>
                        <th>Volume</th>
                    </thead>
                    <tbody>
                      <%  price.forEach((inner)=>{ %>
                            <tr>
                                <td><%= inner.date %></td>
                                <td><%= inner.open %></td>
                                <td><%= inner.high %></td>
                                <td><%= inner.low %></td>
                                <td><%= inner.close %></td>
                                <td><%= inner.volume%></td>
                            </tr>
                      <%  }) %>
                        
                    </tbody>
                </table>
                <br/>
                <h6>TSLA : TESLA Inc.</h6>
                <table>
                    <thead>
                        <th>Date</th>
                        <th>Open</th>
                        <th>High</th>
                        <th>Low</th>
                        <th>Close</th>
                        <th>Volume</th>
                    </thead>
                    <tbody>
                      <%  tesla.forEach((inner)=>{ %>
                            <tr>
                                <td><%= inner.date %></td>
                                <td><%= inner.open %></td>
                                <td><%= inner.high %></td>
                                <td><%= inner.low %></td>
                                <td><%= inner.close %></td>
                                <td><%= inner.volume%></td>
                            </tr>
                      <%  }) %>
                        
                    </tbody>
                </table>
            </div>              
        </div>
          </div>
      </div>
      <footer>
        <h5>Copywright @2020</h5>
     </footer>
      
</body>

</html>